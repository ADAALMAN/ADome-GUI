function render = AdomeStructure()
    %% Generate geodesic ADome structure
    [A, xy] = bucky;         % Generate bucky matrix 
    [i, j] = find(triu(A));  
    B = permute(cat(3, xy(i, :),xy(j, :)), [3 1 2]);
    
    % Plot upper-part bucky 
    length = 1:40;
    render = plot3(B(:, length, 1), B(:, length, 2),B(:, length, 3));
    hold on
end