function outputTextbox(TextArea, cmdData, start)
    persistent firstLine;
    persistent endlines;

    if isempty(firstLine)
        firstLine = 1;
        endlines = 0;
    end

    a = firstLine;
    if (a == 1)
        while (strcmp(cmdData{a}, '#end#') == false)
            data = cmdData{a};
            TextArea.Value{a} = data{a};

    scroll(TextArea, 'bottom')
end